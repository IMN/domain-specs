{
  "info" : {
    "version" : "1.0",
    "title" : "IMN - Merchant Events",
    "description" : "IMN - Merchant Events"
  },
  "x-beezup-ops" : {
    "repo" : "IMN",
    "appRoot" : "TBD",
    "apiName" : "TBD",
    "namespace" : "IMN.MER.Events",
    "comment" : "IMN.MER.Events\n"
  },
  "definitions" : {
    "merchantCreated" : {
      "x-format" : "Event",
      "type" : "object",
      "description" : "When the merchant creates his account on IMN",
      "required" : [ "merchantCode", "companyName", "email" ],
      "properties" : {
        "merchantCode" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/merchantCode"
        },
        "companyName" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/merchantCompanyName"
        },
        "email" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/email"
        }
      }
    },
    "merchantMarketplaceCredentialConfigured" : {
      "x-format" : "Event",
      "allOf" : [ {
        "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/merchantMarketplaceBase"
      }, {
        "type" : "object",
        "required" : [ "merchantCode", "marketplaceCode", "credential" ],
        "properties" : {
          "credential" : {
            "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/marketplaceApiCredential"
          }
        }
      } ]
    },
    "merchantMarketplaceSubscriptionRequested" : {
      "x-format" : "Event",
      "description" : "When the merchant marketplace subscription has been requested",
      "allOf" : [ {
        "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/merchantMarketplaceBase"
      } ]
    },
    "merchantMarketplaceSubscriptionAccepted" : {
      "x-format" : "Event",
      "description" : "When the marketplace accept the mechant subscription request",
      "allOf" : [ {
        "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/merchantMarketplaceBase"
      } ]
    },
    "merchantMarketplaceSubscriptionRefused" : {
      "x-format" : "Event",
      "description" : "When the marketplace refuse the subscription requested by the merchant",
      "allOf" : [ {
        "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/merchantMarketplaceBase"
      }, {
        "type" : "object",
        "required" : [ "merchantCode", "marketplaceCode", "reason" ],
        "properties" : {
          "reason" : {
            "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/merchantMarketplaceSubscriptionRefusalReason"
          }
        }
      } ]
    },
    "merchantProjectionReminded" : {
      "x-format" : "Event",
      "description" : "The merchant projection to database is reminded",
      "allOf" : [ {
        "type" : "object",
        "required" : [ "merchantCode", "senderMessageId" ],
        "properties" : {
          "merchantCode" : {
            "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/merchantCode"
          },
          "senderMessageId" : {
            "type" : "string",
            "format" : "MessageId",
            "description" : "The sender message Id"
          }
        }
      } ]
    },
    "merchantProjected" : {
      "x-format" : "Event",
      "description" : "The merchant state is projected to database",
      "allOf" : [ {
        "type" : "object",
        "required" : [ "merchantCode" ],
        "properties" : {
          "merchantCode" : {
            "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/merchantCode"
          }
        }
      } ]
    },
    "merchantMarketplaceOfferImportationEnabled" : {
