      } ]
    },
    "merchantMarketplaceOfferPublicationEnabled" : {
      "x-format" : "Event",
      "allOf" : [ {
        "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/merchantMarketplaceBase"
      } ]
    },
    "merchantMarketplaceOfferPublicationDisabled" : {
