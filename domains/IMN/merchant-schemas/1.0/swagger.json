{
  "info" : {
    "version" : "1.0",
    "title" : "IMN - Merchant Schemas",
    "description" : "IMN - Merchant Schemas"
  },
  "x-beezup-ops" : {
    "repo" : "IMN",
    "appRoot" : "IMN.MER.Schemas",
    "apiName" : "TBD",
    "namespace" : "IMN.MER.Schemas",
    "comment" : "IMN.MER.Schemas\n",
    "using" : [ "IMN.Schemas" ]
  },
  "definitions" : {
    "marketplaceSubcriptionStatus" : {
      "description" : "The subscription status",
      "type" : "string",
      "enum" : [ "notRequested", "requested", "processing", "accepted", "refused" ],
      "example" : "accepted"
    },
    "merchantMarketplaceSubscriptionRefusalReason" : {
      "type" : "string",
      "minLength" : 5,
      "description" : "The marketplace reason for the subscription's refusal"
    },
    "merchantCompanyName" : {
      "type" : "string",
      "minLength" : 2,
      "description" : "Merchant's company name"
    },
    "markeplaceCredentialStatus" : {
      "description" : "The marketplace credential status",
      "type" : "string",
      "enum" : [ "configured", "notConfigured", "expired" ],
      "example" : "configured"
    },
    "marketplaceTransportSettingsStatus" : {
      "description" : "The marketplace transport settings status",
      "type" : "string",
      "enum" : [ "configured", "notConfigured" ]
    },
    "marketplaceTransportSettingList" : {
      "type" : "array",
      "items" : {
        "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/marketplaceTransportSetting"
      }
    },
    "marketplaceTransportSetting" : {
      "type" : "object",
      "required" : [ "transportType", "shippingCharge", "additionalShippingCharge" ],
      "properties" : {
        "transportType" : {
          "type" : "string",
          "example" : "Standard"
        },
        "shippingCharge" : {
          "type" : "number",
          "example" : 3.35
        },
        "additionalShippingCharge" : {
          "type" : "number",
          "example" : 1.52
        }
      }
    },
    "marketplacePricingSetting" : {
      "type" : "object",
      "required" : [ "ruleType" ],
      "properties" : {
        "ruleType" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/marketplacePricingRuleType"
        },
        "value" : {
          "type" : "number",
          "example" : 42.01
        }
      }
    },
    "marketplacePricingRuleList" : {
      "type" : "array",
      "items" : {
        "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/marketplacePricingRule"
      }
    },
    "marketplacePricingRule" : {
      "type" : "object",
      "required" : [ "ruleType" ],
      "properties" : {
        "ruleType" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/marketplacePricingRuleType"
        },
        "operatorType" : {
          "type" : "string",
          "enum" : [ "plus", "minus" ],
          "example" : "plus"
        },
        "unit" : {
          "type" : "string",
          "enum" : [ "percent", "amount" ],
          "example" : "percent"
        }
      }
    },
    "marketplacePricingRuleType" : {
      "type" : "string",
      "default" : "SamePricing",
      "enum" : [ "PercentIncreasePricing", "PercentDecreasePricing", "AmountIncreasePricing", "AmountDecreasePricing", "SamePricing" ],
      "x-lov" : "MarketplacePricingRuleType"
    },
    "marketplaceInfo" : {
      "type" : "object",
      "required" : [ "code", "imageUrl", "subscriptionStatus", "credentialStatus", "transportSettingsStatus", "source" ],
      "properties" : {
        "code" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/marketplaceCode"
        },
        "imageUrl" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/imn.io/v1#/definitions/httpUrl"
        },
        "subscriptionStatus" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/marketplaceSubcriptionStatus"
        },
        "credentialStatus" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/markeplaceCredentialStatus"
        },
        "transportSettingsStatus" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/marketplaceTransportSettingsStatus"
        },
        "source" : {
          "type" : "boolean",
          "description" : "If true, this marketplace is the source of offers",
          "default" : false,
          "example" : false
        }
      }
    },
    "marketplaceSubcriptionReporting" : {
      "type" : "object",
      "required" : [ "status", "messages" ],
      "properties" : {
        "status" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/marketplaceSubcriptionStatus"
        },
        "messages" : {
          "type" : "array",
          "items" : {
            "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/marketplaceSubcriptionMessage"
          }
        }
      }
    },
    "marketplaceSubcriptionMessage" : {
      "type" : "object",
      "required" : [ "senderType", "utcDate", "status" ],
      "properties" : {
        "senderType" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/marketplaceSubcriptionMessageSenderType"
        },
        "utcDate" : {
          "type" : "string",
          "format" : "date-time",
          "example" : "2009-06-15T13:45:30Z"
        },
        "status" : {
          "$ref" : "https://api.swaggerhub.com/domains/IMN/merchant-schemas/1.0#/definitions/marketplaceSubcriptionStatus"
        },
        "message" : {
          "type" : "string",
          "example" : "Wrong shoes!"
        }
      }
    },
    "marketplaceSubcriptionMessageSenderType" : {
      "type" : "string",
      "enum" : [ "merchant", "marketplace" ],
      "example" : "marketplace"
    }
  }
}